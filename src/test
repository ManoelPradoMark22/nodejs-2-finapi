const target = { id: "1", name: "mango", color: "yellow"};
console.log(Object.entries(target));


const target2 = { id: "1"};
const keyValue = Object.entries(target2)
console.log(keyValue);
console.log(keyValue[0][0])
console.log(keyValue[0][1])

const arr = [1, 2, 3]

arr.push(4);

console.log(arr);

const teste = {};
(
  teste
  && Object.keys(teste).length !== 0
  && Object.getPrototypeOf(teste) === Object.prototype
) ? console.log(true) : console.log(false);

