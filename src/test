const sufix = ['@gmail.com', '@hotmail.com', '@outlook.com', '@accenture.com', '@yahoo.com'];
console.log(sufix);
const indexSufix = Math.floor(Math.random() * sufix.length);
console.log(indexSufix);
const email = `dasdasd${sufix[indexSufix]}`;

console.log(email);